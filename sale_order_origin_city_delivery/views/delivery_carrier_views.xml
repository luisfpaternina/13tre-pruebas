<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="view_delivery_carrier_citie_from_form" model="ir.ui.view">
			<field name="name">view.delivery.carrier.cities.form</field>
			<field name="model">delivery.carrier</field>
			<field name="inherit_id" ref="delivery.view_delivery_carrier_form"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='zip_from']" position="before">
					<field name="town_from_ids" widget="many2many_tags"/>
				</xpath>
			</field>
		</record>
	</data>

	<data>
		<record id="view_delivery_carrier_citie_to_group_form" model="ir.ui.view">
			<field name="name">view.delivery.carrier.cities.to.group.form</field>
			<field name="model">delivery.carrier</field>
			<field name="inherit_id" ref="sale_delivery_carrier_cities.view_delivery_carrier_citie_to_form"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='town_to_ids']" position="replace">
				</xpath>
				<xpath expr="//field[@name='zip_to']" position="before">
					<field name="town_to_ids" string="town to" widget="many2many_tags"/>
				</xpath>
			</field>
		</record>
	</data>
</odoo>
 