<odoo>
  <data>
    <record model="ir.ui.view" id="account_flat_file_davivienda_view">
      <field name="name">account_flat_file_davivienda_view</field>
      <field name="model">account.flat.file.base</field>
      <field name="inherit_id" ref="account_flat_file_base.account_flat_file_base_wizard_form"/>
      <field name="arch" type="xml">
        <xpath expr="group[@name='file']" position="before">
          <group>
            <group>
              <field name="service_code" attrs="{'required':[('bank_bic','=','51'),('file_type','=','get_collect_data_bank')],'invisible':['|',('bank_bic','!=','51'),('file_type','!=','get_collect_data_bank')]}"/>
            </group>
            <group>
              <field name="subservice_code" attrs="{'required':[('bank_bic','=','51'),('file_type','=','get_collect_data_bank')],'invisible':['|',('bank_bic','!=','51'),('file_type','!=','get_collect_data_bank')]}"/>
            </group>
          </group>
        </xpath>
      </field>
    </record>
  </data>
</odoo>