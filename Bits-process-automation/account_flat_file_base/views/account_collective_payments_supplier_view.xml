        <field name="name">account.collective.payments.supplier.flat.file.form.view</field>
        <field name="model">account.collective.payments.supplier</field>
        <field name="inherit_id" ref="account_collective_payments_supplier.account_collective_payments_supplier_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form/sheet" position="before">
                <header>
                    <button name="generate_flat_file" string="Generate Flat File" type="object" class="oe_highlight"/>
                </header>
            </xpath>
        </field>
    </record>
</odoo>