<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
	    <record id="ir_cron_get_document_invoice" model="ir.cron">
	        <field name="name">FE: Get documents for invoices</field>
	        <field name="model_id" ref="model_account_move"/>
	        <field name="state">code</field>
	        <field name="code">model.action_cron_get_document_invoices()</field>
	        <field name="interval_number">10</field>
	        <field name="interval_type">minutes</field>
	        <field name="numbercall">-1</field>
	        <field name="active" eval="True"/>
	    </record>
		<record id="ir_cron_get_status_inquiry_deadline" model="ir.cron">
	        <field name="name">FE: Get status inquiry deadline</field>
	        <field name="model_id" ref="model_account_move"/>
	        <field name="state">code</field>
	        <field name="code">model.action_cron_get_acceptance_status()</field>
	        <field name="interval_number">5</field>
	        <field name="interval_type">minutes</field>
	        <field name="numbercall">-1</field>
	        <field name="active" eval="True"/>
	    </record>
		<record id="ir_cron_validate_final_acceptance_status" model="ir.cron">
	        <field name="name">FE: Validate final acceptance status of the invoice</field>
	        <field name="model_id" ref="model_account_move"/>
	        <field name="state">code</field>
	        <field name="code">model.validate_final_acceptance_status()</field>
	        <field name="interval_number">6</field>
	        <field name="interval_type">hours</field>
	        <field name="numbercall">-1</field>
	        <field name="active" eval="True"/>
	    </record>
    </data>
</odoo>
