#. module: stock_income
#: model:mail.template,body_html:stock_income.send_confirmation_suppliers_email_template
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;"
"\">\n"
"                        Sirs;<br/>\n"
"                            ${object.partner_id.name}<br/>\n"
"                        Good day,<br/><br/>\n"
"\n"
"                        Warehouse Entry Number <strong>${object.name}</"
"strong> is attached confirming receipt of the service or products "
"purchased.\n"
"                        ${object.env.company.name} issues a purchase order "
"for the services provided or the goods purchased, please\n"
"                        Reference the Warehouse Entry Number generated at "
"the time of invoicing, or it is allowed to send purchase orders from "
"another email and / or internal user of the company.<br/>\n"
"                        If you have any questions, feel free to contact us."
"<br/>\n"
"                        Sincerely,<br/><br/>\n"
"\n"
"                        ${object.env.company.name}.\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;"
"\">\n"
"                        Señores;<br/>\n"
"                            ${object.partner_id.name}<br/>\n"
"                        Buen día,<br/><br/>\n"
"\n"
"                        Se adjunta Entrada de Almacén No <strong>${object."
"name}</strong> Confirmando la recepción del servicio o productos "
"adquiridos.\n"
"                        ${object.env.company.name} emite orden de compra "
"por los servicios prestados o los bienes adquiridos, favor\n"
"                        referenciar el No de Entrada de Almacén generado al "
"momento de facturar. O se permite enviar órdenes de compra desde otro "
"correo y / o usuario interno de la empresa.<br/>\n"
"                        Si tiene alguna pregunta, no dude en contactarnos."
"<br/>\n"
"                       Atentamente,<br/><br/>\n"
"\n"
"                        ${object.env.company.name}.\n"
"                    </p>\n"
"                </div>\n"
"            "

