<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>

    <!--Begin View Tree-->
    <record id="hr_recruitment_level_tree" model="ir.ui.view">
        <field name="name">hr_recruitment_level_tree</field>
        <field name="model">hr.recruitment.level</field>
        <field name="arch" type="xml">

            <tree>
                <field name="name" />
            </tree>

        </field>
    </record>
    <!--End View Tree-->

    <!--Begin View Form-->
    <record id="hr_recruitment_level_form" model="ir.ui.view">
      <field name="name">hr_recruitment_level_form</field>
      <field name="model">hr.recruitment.level</field>
      <field name="arch" type="xml">

        <form string="Recruitment aptitudes level" class="o_form_project_tasks">
          <sheet>
            <group>
              <group>
                <field name="name"/>
              </group>
            </group>
          </sheet>
        </form>

      </field>
    </record>
    <!--End View Form-->

  </data>
</odoo>