#. module: bits_purchase_order_template
#: model:mail.template,subject:bits_purchase_order_template.email_template_custom_purchase_done
msgid "${object.company_id.name} Order (Ref ${object.name or 'n/a' })"
msgstr "${object.company_id.name} Orden (Ref ${object.name or 'n/a' })"

#. module: bits_purchase_order_template
#: model:ir.actions.report,print_report_name:bits_purchase_order_template.action_report_custom_purchase_order
msgid "('Purchase Order - %s' % (object.name))"
msgstr "('Orden de compra - %s' % (object.name))"

#. module: bits_purchase_order_template
#: model:mail.template,body_html:bits_purchase_order_template.email_template_notify_budget_excess
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        Receive a cordial greeting, please require approval \n"
"                        of budget increase to make the purchase request. <strong>${object.name}</strong>\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        Reciba un cordial saludo, por favor se requiere aprobación \n"
"                        de aumento de presupuesto para realizar la solicitud de compra. <strong>${object.name}</strong>\n"
"                    </p>\n"
"                </div>\n"
"            "

#. module: bits_purchase_order_template
#: model:mail.template,body_html:bits_purchase_order_template.email_template_custom_purchase_done
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Dears\n"
"        <br/>\n"
"        <strong> ${object.partner_id.name}</strong>\n"
"        <br/>\n"
"        % if object.partner_id.parent_id:\n"
"            <strong>(${object.partner_id.parent_id.name})</strong>\n"
"        % endif\n"
"        Good day,\n"
"        <br/>\n"
"        <br/>\n"
"        Attached purchase order No. <strong>${object.name}</strong>\n"
"        % if object.partner_ref:\n"
"            with reference: ${object.partner_ref}\n"
"        % endif\n"
"        , please keep in mind that <strong>${object.company_id.name}</strong>\n"
"        issue purchase order for the services provided or the goods purchased, \n"
"        this document will be sent to your email directly by the financial area,\n"
"         it is <strong>NOT</strong> allowed to send purchase orders from \n"
"         other mail and/or internal user of the company.\n"
"        <br/>\n"
"        If you have any questions, please do not hesitate to contact us.\n"
"        <br/>\n"
"        Best regards,\n"
"    </p>\n"
"</div>"
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Señores\n"
"        <br/>\n"
"        <strong> ${object.partner_id.name}</strong>\n"
"        <br/>\n"
"        % if object.partner_id.parent_id:\n"
"            <strong>(${object.partner_id.parent_id.name})</strong>\n"
"        % endif\n"
"        Buen día,\n"
"        <br/>\n"
"        <br/>\n"
"        Adjunto orden de compra No. <strong>${object.name}</strong>\n"
"        % if object.partner_ref:\n"
"            referencia: ${object.partner_ref}\n"
"        % endif\n"
"        , favor tener presente que <strong>${object.company_id.name}</strong> emite orden de compra por los servicios prestados o las mercancías adquiridas, este documento será remitido a su correo directamente por el área financiera, <strong>NO</strong> está permitido remitir órdenes de compra desde otro correo y/o usuario interno de la compañía.\n"
"        <br/>\n"
"        Si tiene alguna pregunta, no dude en contactarnos.\n"
"        <br/>\n"
"        Atentamente,\n"
"    </p>\n"
"</div>"

