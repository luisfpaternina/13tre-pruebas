<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="hr_recruitment_scheduling_stage_view" model="ir.ui.view">
            <field name="name">hr_recruitment_scheduling_stage_view</field>
            <field name="model">hr.recruitment.stage</field>
            <field name="inherit_id" ref="hr_recruitment.hr_recruitment_stage_form" />
            <field name="arch" type="xml">

                <xpath expr="//field[@name='job_ids']" position="after">
                    <field name="assigned_to" />
                </xpath>
                
            </field>
        </record>
    </data>
</odoo>