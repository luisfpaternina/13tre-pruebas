<odoo>
    <data>

        <record model="ir.ui.view" id="account_bank_statement_conciliation_setting">
            <field name="name">account_bank_statement_conciliation_setting</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="account.res_config_settings_view_form" />
            <field name="arch" type="xml">

                <xpath expr="//div[@id='analytic']" position="after">

                    <h2>Account Bank Statement Conciliation</h2>
                    <div class="row mt16 o_settings_container">
                        <div class="col-lg-6 col-md-6 o_setting_box">
                            <div class="o_setting_right_pane">
                                <label for="conciliation_type" class="o_form_label"/>
                                <br></br>
                                <field name="conciliation_type"/>
                            </div>
                        </div>
                    </div>

                </xpath>
                
            </field>
        </record>


    </data>
</odoo>