<?xml version="1.0" encoding="UTF-8"?>     
<odoo>
    <data>

       <record id="general_budgets_analysis_report_view" model="ir.ui.view">
           <field name="name">Print General Budget report</field>
           <field name="model">general.budget.analysis</field>
           <field name="arch" type="xml">
               <tree string="General Budgets Analysis" create="0" edit="0">
                   <field name="crossovered_budget_state" invisible="1"/>
                   <field name="general_budget_id"/>
                   <field name="analytic_account_id"/>
                   <field name="company_id"/>
                   <field name="date_from"/>
                   <field name="date_to" />
                   <field name="paid_date"/>
                   <field name="planned_amount"/>
                   <field name="practical_amount"/>
                   <field name="theoritical_amount"/>
               </tree>
           </field>
       </record>

       <record id="view_general_budgets_analysis_search" model="ir.ui.view">
            <field name="name">general.budget.analysis.search</field>
            <field name="model">general.budget.analysis</field>
            <field name="arch" type="xml">
                <search string="Budget Lines">
                    <field name="analytic_account_id"/>
                    <filter name="filter_not_cancelled" string="Not Cancelled" domain="[('crossovered_budget_state','!=','cancel')]"/>
                </search>
            </field>
        </record>

       <record id="action_menu_general_budgets_analysis" model="ir.actions.act_window">
           <field name="name">General Budgets Analysis</field>
           <field name="res_model">general.budget.analysis</field>
           <field name="view_mode">list</field>
           <field name="view_id" ref="general_budgets_analysis_report_view"/>
           <field name="context">{'search_default_filter_not_cancelled':True}</field>
       </record>

       <menuitem
        name="General Budget  Analysis Report"
        parent="account.account_reports_management_menu"
        id="main_menu_general_budgets_analysis"
        sequence="21"
        action="account_budget_analysis_report.action_menu_general_budgets_analysis"/>
        
    </data>
</odoo>