<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_hr_employee_internal_request_inherit" model="ir.ui.view">
            <field name="name">view.hr.employee.internal.request.inherit</field>
            <field name="model">hr.employee.internal.request</field>
            <field name="inherit_id" ref="hr_employee_internal_request.hr_employee_internal_request_form"></field>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[2]" position="after">
                    <page string="Applicants">
                        <field name="applicant_line_ids">
                            <tree>
                                <field name="name" />
                                <field name="identification_number" />
                            </tree>
                        </field>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
