<odoo>
    <data>
      <report
        id="l10n_co_e_invoice_format_report"
        model="account.move"
        string="Custom Invoice Report"
        report_type="qweb-pdf"
        name="l10n_co_e_invoice_format.invoice_format_template"
        file="l10n_co_e_invoice_format.invoice_format_template"
        attachment_use = "True"
        attachment="(object.state == 'posted') and ((object.name or 'INV').replace('/','_')+'_FE_INVOICE.pdf')"
        print_report_name="(object._get_report_base_filename())"
      />
    </data>
</odoo>