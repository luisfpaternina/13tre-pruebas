<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_account_move_inherit" model="ir.ui.view">
        <field name="name">account.move.form.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">

            <xpath expr="//field[(@name='partner_id')]" position="after">
                <field name="is_validate"/>
                <field name="is_validate_date"/>
                <field name="aditional_value"/>
                <button string="Validate" name="_validate_subscription" type="object" invisible="1"/>
            </xpath>
            
        </field>
    </record>
</odoo>     